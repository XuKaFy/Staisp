DEFVAR i32:n 50

DEFFUNC i32:fib ( i32:n ) BLOCK {
    IF OR EQ n i32:1
          EQ n i32:2
        RETURN i32:1
    RETURN ADD fib ( SUB n 1 )
               fib ( SUB n 2 )
}

DEFFUNC main ( ) BLOCK {
    ASSIGN n 10
    DEFVAR i32:ans 10
    WHILE ULE n 20
    ASSIGN n ADD n 1
    BLOCK {
        ASSIGN ans fib ( n )
    }
    RETURN ans
}
