# fib program # fib

DEFVAR i32:n 1

DEFFUNC i32:fib(i32:n) BLOCK {
    IF OR EQ n 1
          EQ n 2
        RETURN 1
    RETURN ADD fib (SUB n 1)
               fib (SUB n 2)
}

DEFFUNC i32:main() BLOCK {
    DEFVAR i32:ans 0
    ASSIGN n 5
    WHILE ULE n 10
        ASSIGN n ADD n 1 # add n to 10
    BLOCK {
        ASSIGN ans fib (n)
    }
    RETURN ans
}

#end fib program
